<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--这里的namespace还没有确定-->
<mapper namespace="com.ezd.dao.*">
    <resultMap id="erlbigret" type="com.ezd.model.EzdErlbigret"><!--大招会报名表-->
        <id column="erlbigret_id" property="erlbigretId" /><!--编号-->
        <result column="erlbigret_time" property="erlbigretTime" /><!--报名时间-->
        <association property="erlbigretUser" resultMap="*.umg"/><!--谁报名的这个大招会-->
        <association property="erlbigretMes" resultMap="*.bigret"/><!--哪个大招会-->
        <association property="erlbigretStatus" resultMap="*.restatus"/><!--报名状态-->
    </resultMap>
</mapper>